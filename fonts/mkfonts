./bdfconv.exe -v -f 1 -m '45-57' font_helvR24.bdf -o ../platform.io/src/resources/font_helvR24.h -n font_helvR24
./bdfconv.exe -v -f 1 -m '48-111' font_icons.bdf -o ../platform.io/src/resources/font_icons.h -n font_icons
